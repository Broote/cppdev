<!DOCTYPE html>
<html>
<head>
  <title>Cpp</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag 'jquery', 'nested_form', :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
<div id="user_nav">
  <% if user_signed_in? %>
    Signed_in as <%= current_user.email %>. Not you?
    <%= link_to "Signed_out", destroy_user_session_path %>
  <%  else %>
    <%= link_to "Signed_up", new_user_registration_path %> or <%= link_to "sign in", new_user_session_path%>
  <%  end %>
</div>

<%= yield %>

</body>
</html>
